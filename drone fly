from djitellopy import tello
from time import sleep

me = tello.Tello()
me.connect()

print(me.get_battery())

me.takeoff()

# Flies up 170cm and then forward 480cm
me.move_up(170)
sleep(.5)
me.move_forward(480)
sleep(.5)

# Flies left 315cm and then back 480cm 
me.move_left(315)
sleep(.5)
me.move_back(480)
sleep(.5)

# Flies right 315cm and then down 200cm
me.move_right(315)
sleep(.5)
me.move_down(200)

me.land()
